{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Post",
  "type": "object",
  "required": [
    "author",
    "creation_date",
    "external_id",
    "post_id",
    "post_title",
    "tags",
    "text"
  ],
  "properties": {
    "author": {
      "type": "string"
    },
    "creation_date": {
      "type": "string"
    },
    "editor": {
      "type": [
        "string",
        "null"
      ]
    },
    "external_id": {
      "type": "string"
    },
    "last_edit_date": {
      "type": [
        "string",
        "null"
      ]
    },
    "post_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "post_title": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "text": {
      "type": "string"
    }
  }
}
